{
  "name": "petmatch-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "axios": "^1.5.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "concurrently \"tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch\" \"react-scripts start\"",
    "build": "tailwindcss -i ./src/index.css -o ./src/tailwind.css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
